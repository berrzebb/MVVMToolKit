<UserControl
    x:Class="WithModuleSample.Views.PopupContentView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:WithModuleSample.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid>
        <Border Background="Aquamarine" />
        <StackPanel>
            <Button Command="{Binding DoTestCommand}" Content="팝업테스트" />
            <Button Command="{x:Static SystemCommands.MinimizeWindowCommand}" Content="최소화테스트" />
        </StackPanel>
    </Grid>
</UserControl>
