<UserControl
    x:Class="WithModuleSample.RegionableView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:WithModuleSample"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:zones="clr-namespace:MVVMToolKit.Navigation.Zones;assembly=MVVMToolKit"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">

    <StackPanel>
        <Button Height="20" Command="{Binding SwitchRegionCommand}">
            Switch
        </Button>
        <ContentControl zones:ZoneRegistry.ZoneName="RegionZone" />

    </StackPanel>


</UserControl>
